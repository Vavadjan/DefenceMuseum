kit-appearance: none;\n  -moz-appearance: none;\n  appearance: none;\n  flex: 1;\n  margin-right: 10px;\n  height: 8px;\n  background: transparent;\n  overflow: hidden;\n  cursor: pointer;\n}\n.audio-player__progress-control::-moz-range-track {\n  height: 8px;\n  background: linear-gradient(90deg, white 0%, white var(--progress, 0%), transparent var(--progress, 0%), transparent 100%);\n  background-color: #717171;\n  border-radius: 10px;\n}\n.audio-player__progress-control::-moz-range-thumb {\n  width: 0px;\n  height: 20px;\n  background: transparent;\n  border-radius: 50%;\n  border-color: transparent;\n}\n.audio-player__progress-control::-webkit-slider-runnable-track {\n  -webkit-appearance: none;\n  height: 8px;\n  background: linear-gradient(90deg, white 0%, white var(--progress, 0%), transparent var(--progress, 0%), transparent 100%);\n  background-color: #717171;\n  border-radius: 10px;\n}\n.audio-player__progress-control::-webkit-slider-thumb {\n  display: flex;\n  align-items: center;\n  -webkit-appearance: none;\n  position: relative;\n  top: -9px;\n  left: 0;\n  width: 0px;\n  height: 20px;\n  background: white;\n  border-radius: 50%;\n  border-color: transparent;\n  z-index: 0;\n}\n.audio-player__progress-timer {\n  font-size: 14px;\n  line-height: 24px;\n  letter-spacing: 0.01em;\n  text-align: center;\n  padding: 0 9px;\n}\n\n.audio-player-track-list {\n  display: grid;\n  gap: 12px;\n}\n.audio-player-track-list__item {\n  font-size: 20px;\n  line-height: 24px;\n  letter-spacing: 0.01em;\n  border-radius: 6px;\n  padding: 4px 6px;\n  cursor: pointer;\n  transition: background-color 0.4s, color 0.4s;\n}\n.audio-player-track-list__item_active {\n  color: #0078FF;\n  background: rgba(255, 255, 255, 0.3);\n}\n\n.player-btn {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  fill: white;\n  transition: fill 0.4s;\n}\n.player-btn > svg {\n  width: 32px;\n  height: 36px;\n}\n.player-btn_nav {\n  width: 32px;\n  height: 32px;\n  min-width